{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"options": {
		"cwd": "${workspaceRoot}",
		"env": {
			"NODE_ENV": "development",
			"BUILD_TARGET": "local"
		}
	},
	"tasks": [
		{
			"type": "npm",
			"script": "build",
			"group": "build",
			"problemMatcher": {
				"base": "$tsc",
				"fileLocation": "relative"
			},
			"label": "build",
			"detail": "tsc"
		}
	]
}
